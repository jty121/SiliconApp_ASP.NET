@{
    ViewData["Title"] = "Page not found!";
}

<section class="notfound">
    <div class="container">
        <div class="content">
            <img src="~/images/image/404.svg" alt="404, page not found">
            <span class="d5">Ooops!</span>
            <p>The page you are looking for is not available.</p>
            <a class="btn-theme" asp-controller="Home" asp-action="Index"><i class="fa-regular fa-house"></i>Go to homepage</a>
        </div>
    </div>
</section>
